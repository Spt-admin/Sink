{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "from",
  "main": "dist/server/index.mjs",
  "assets": {
    "binding": "ASSETS",
    "directory": "dist/public"
  },
  "compatibility_date": "2025-05-08",
  "compatibility_flags": [
    "nodejs_compat"
  ],
 // --- BLOCO DE OBSERVABILIDADE ADICIONADO AQUI ---
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  // ---------------------------------------------
  "keep_vars": true,
  "upload_source_maps": true,
  "ai": {
    "binding": "AI"
  },
  "analytics_engine_datasets": [
    {
      "binding": "ANALYTICS",
      "dataset": "sink"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "fa394db5c0fb45f8af8b677a8cc7f431" // IMPORTANT: Change this to your KV namespace ID.
    }
  ]
}
